{
    "templates_dir": "templates",
    "output_dir": "{{ project.name }}",
    "context": {
        "project": {
            "name": "example"
        }
    },
    "alias": {
        "example": {
            "template_dir": "{{ project.name }}",
            "output_dir": "{{ module }}",
            "context": "module:name",
            "dir": false
        }
    }
}